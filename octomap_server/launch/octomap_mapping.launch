<launch>
<arg name="ns" default="J0" />
<node ns="$(arg ns)" pkg="octomap_server" type="octomap_server_node" name="octomap_server">
		<param name="resolution" value="0.05" />
		<param name="filter_ground" value="false" />
		<param name="occupancy_min_z" value="0.02" />
		<!-- <param name="occupancy_max_z" value="false" /> -->
		<!-- <param name="ground_filter/plane_distance" value="10" /> -->
		<!-- <param name="ground_filter/distance" value="10" /> -->
		<param name="frame_id" type="string" value="$(arg ns)/map_locus" />
		<remap from="cloud_in" to="/$(arg ns)/locus/octree_map" />
		<!-- <remap from="cloud_in" to="/$(arg ns)/velodyne_points" /> -->
		<!-- <remap from="projected_map" to="/$(arg ns)/octree_projected_map" />	 -->
		<remap from="projected_map" to="/$(arg ns)/locus/octree_projected_map" />	
	</node>
</launch>